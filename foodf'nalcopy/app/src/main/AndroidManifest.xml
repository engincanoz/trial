<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"

        tools:targetApi="31">
        <activity
            android:name=".recylerviewrow"
            android:exported="false"
            android:label="@string/title_activity_recylerviewrow"
             />
        <activity
            android:name=".recyclerview"
            android:exported="false"
            android:label="@string/title_activity_recyclerview"
           />
        <activity
            android:name=".ocrScan"
            android:exported="false"
            android:label="@string/title_activity_ocr_scan"
           />
        <activity
            android:name=".addProduct"
            android:exported="false"
            android:label="@string/title_activity_add_product" />
        <activity
            android:name=".dietDisplay"
            android:exported="false"
            android:label="@string/title_activity_diet_display" />
        <activity
            android:name=".dietHelp"
            android:exported="false"
            android:label="@string/title_activity_diet_help" />
        <activity
            android:name=".productx"
            android:exported="false"
            android:label="@string/title_activity_product" />
        <activity
            android:name=".expiredProducts"
            android:exported="false"
            android:label="@string/title_activity_expired_products" />
        <activity
            android:name=".myProducts"
            android:exported="false"
            android:label="@string/title_activity_my_products" />
        <activity
            android:name=".login"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="false"
            android:label="@string/title_activity_login"
             />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>